{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "6.10"
        }
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-root-import",
      [
        {
          "rootPathPrefix": "~",
          "rootPathSuffix": "src"
        },
        {
          "rootPathPrefix": "#",
          "rootPathSuffix": "test"
        }
      ]
    ],
    "transform-object-rest-spread"
  ]
}
